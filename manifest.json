{
    "name": "Download Organiser",
    "version": "0.2",
    "description": "Organizes .pcap files by today's date. Format: DD-MM-YYYY",
    "background": {
        "service_worker": "bg.js"},
    "permissions": ["downloads"],
    "manifest_version": 3,
    "icons": { "16": "icons/16.png",
        "48": "icons/48.png",
       "128": "icons/128.png" },
    "content_security_policy": {
        "extension_pages":"script-src 'self'; default-src 'self'"
    }
}
